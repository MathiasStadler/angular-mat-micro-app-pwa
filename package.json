{
  "name": "angular-mat-micro-app-pwa",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "http-server -c-1 dist/angular-mat-micro-app-pwa",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "build:clients": "npm run build:blog",
    "build:clients_par": "concurrently \"npm run build:chat\" \"npm run build:blog\"",
    "build:chat_prod": "ng build --prod --project web-component-chat --output-hashing none --vendor-chunk false --output-path dist/angular-mat-micro-app-pwa/web-component-chat",
    "build:blog_prod": "ng build --prod --project blog --output-hashing none --vendor-chunk false --output-path dist/angular-mat-micro-app-pwa/blog",
    "build:chat": "ng build --prod --project web-component-chat --output-hashing none --vendor-chunk false --output-path dist/angular-mat-micro-app-pwa/web-component-chat",
    "build:blog": "ng build  --prod --project blog --output-hashing none --vendor-chunk false --output-path dist/angular-mat-micro-app-pwa/blog",
    "build:chat_s": "ng build --no-aot --build-optimizer=false --project web-component-chat --output-hashing none --vendor-chunk false --output-path dist/angular-mat-micro-app-pwa/web-component-chat",
    "build:blog_s": "ng build  --no-aot --build-optimizer=false --project blog --output-hashing none --vendor-chunk false --output-path dist/angular-mat-micro-app-pwa/blog",
    "build:shell_prod": "ng build --project angular-mat-micro-app-pwa --prod",
    "build:shell": "ng build --project angular-mat-micro-app-pwa ",
    "build": "npm run build:shell && npm run build:clients",
    "build_par": "concurrently \"npm run build:shell\" \"npm run build:clients\""
  },
  "private": true,
  "dependencies": {
    "@angular-devkit/build-angular": "^0.8.4",
    "@angular-devkit/build-optimizer": "^0.8.4",
    "@angular/animations": "^6.1.9",
    "@angular/cdk": "^6.4.7",
    "@angular/common": "^6.1.9",
    "@angular/compiler": "^6.1.9",
    "@angular/core": "^6.1.9",
    "@angular/elements": "^6.1.9",
    "@angular/forms": "^6.1.9",
    "@angular/http": "^6.1.9",
    "@angular/material": "^6.4.7",
    "@angular/platform-browser": "^6.1.9",
    "@angular/platform-browser-dynamic": "^6.1.9",
    "@angular/pwa": "^0.8.4",
    "@angular/router": "^6.1.9",
    "@angular/service-worker": "^6.1.9",
    "@webcomponents/custom-elements": "^1.2.1",
    "bootstrap": "^4.1.3",
    "core-js": "^2.5.7",
    "idb": "^2.1.3",
    "ngx-build-plus": "^6.1.0",
    "rxjs": "6.3.3",
    "service-worker": "0.0.0",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.8.0",
    "@angular/cli": "^6.2.4",
    "@angular/compiler-cli": "^6.1.9",
    "@angular/language-service": "^6.1.9",
    "@types/jasmine": "^2.8.8",
    "@types/jasminewd2": "^2.0.4",
    "@types/node": "~10.11.3",
    "codelyzer": "~4.4.4",
    "concurrently": "^4.0.1",
    "copyfiles": "^2.1.0",
    "cpr": "^3.0.1",
    "css-loader": "^1.0.0",
    "html-loader": "^0.5.5",
    "jasmine-core": "~3.2.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~3.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "^2.0.4",
    "karma-jasmine": "^1.1.2",
    "karma-jasmine-html-reporter": "^1.3.1",
    "live-server": "^1.2.0",
    "protractor": "^5.4.1",
    "rollup-plugin-commonjs": "^9.1.8",
    "rollup-plugin-node-resolve": "^3.4.0",
    "rollup-plugin-typescript2": "^0.17.1",
    "rollup-plugin-uglify": "^4.0.0",
    "ts-node": "~7.0.1",
    "tslint": "~5.11.0",
    "typescript": "^2.9.2",
    "webpack": "^4.20.2"
  }
}
